{
  "name": "inventory-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build:css": "npx tailwindcss build -o dist/output.css",
    "watch:css": "npx tailwindcss -i ./public/stylesheets/style.css -o ./dist/output.css --watch",
    "start:dev": "npx concurrently \"nodemon index\" \"npm run watch:css\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^8.0.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.2",
    "tailwindcss": "^3.3.5"
  }
}
